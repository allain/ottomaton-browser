{
  "name": "ottomaton-browser",
  "version": "0.3.2",
  "description": "A library of browser automation actions for use with ottomaton.",
  "main": "index.js",
  "bin": {
    "otto-browser": "bin/otto-browser"
  },
  "scripts": {
    "compile": "babel --stage 2 --optional runtime --source-maps true -d lib/ src/ && chmod +x lib/bin/otto-browser.js",
    "clean": "rm -r lib",
    "pretest": "npm run compile",
    "test": "blue-tape test/*.js | tap-dot",
    "prepublish": "npm run clean && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/allain/ottomaton-browser"
  },
  "keywords": [
    "ottomaton",
    "library",
    "browser",
    "automation"
  ],
  "author": "Allain Lalonde <allain.lalonde@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allain/ottomaton-browser/issues"
  },
  "homepage": "https://github.com/allain/ottomaton-browser#readme",
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "debug": "^2.2.0",
    "fj-flatten": "^1.0.0",
    "ottomaton": "~0.x.x",
    "selenium-webdriver": "^2.47.0"
  },
  "devDependencies": {
    "blue-tape": "^0.1.10",
    "tap-dot": "^1.0.0"
  }
}
